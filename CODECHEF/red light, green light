#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n,k,i;
	    cin>>n>>k;
	    int h[n];
	    for(i=0;i<n;i++){
	        cin>>h[i];
	    }
	    int count=0;
	    for(i=0;i<n;i++){
	        if(k<h[i]){
	            count++;
	        }
	         
	    }
	    cout<<count<<endl;
	}
	return 0;
}
